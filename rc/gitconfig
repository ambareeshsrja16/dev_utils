[user]
  name = Ambareesh
  email = ambareeshsrja16@gmail.com
[color]
  ui = auto
[alias]
  add-remove = !git remove-deleted && git add-untracked
  add-untracked = !git ls-files -o --exclude-standard | xargs git add -v
  adog = log --all --decorate --oneline --graph
  check-all-downstream-status = !git-check-all-downstream-status
  ci = commit
  co = checkout
  cp = cherry-pick
  delete-branches-with-prompt = !git-delete-branches-with-prompt
  first-downstream = !git-first-downstream
  first-upstream = !git-first-upstream
  graft = !git-graft-stack
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
  local-commits = !git-all-local-commits
  ls = branch -a
  mark-landed = !git-mark-branch-landed
  purge = !git clean -fdx
  rebase-all-stacked-diffs = !git-rebase-all-stacked-diffs
  rebase-stacked = !git-rebase-stacked-diff
  remove-deleted = !git ls-files -d | xargs git rm
  show-upstream = !git branch -vv --list $(git rev-parse --abbrev-ref HEAD)
  st = status
  stack = !git-stack
  stack-ahead-by = !git-stack-ahead-by
[core]
  editor = vim
  pager = less -F -x4
[push]
  default = matching
[branch]
  autosetupmerge = true
[gui]
  fontui = -family \"Ubuntu Mono\" -size 5 -weight normal -slant roman -underline 0 -overstrike 0
  fontdiff = -family \"Ubuntu Mono\" -size 5 -weight normal -slant roman -underline 0 -overstrike 0
[pull]
  rebase = false
[url "git@github.com:"]
  insteadOf = https://github.com/
